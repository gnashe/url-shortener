<h1>All Saved Urls</h1>
<table>
  <tr>
    <th>Original Url</th>
    <th>Short Url</th>
  </tr>
  <% @short_urls.each do |short_url| %>
    <tr>
      <td>
        <%= short_url.original_url %>
      </td>
      <td>
        <%= request.protocol + request.host_with_port %>/<%= short_url.short_path %>
      </td>
    </tr>
  <% end %>
</table>
